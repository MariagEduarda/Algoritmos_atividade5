programa {
	funcao inicio() {
	
		caracter letra
		
		escreva("Fale uma letra: ")
		leia(letra)
		
		se ( letra == 'a' ou letra == 'A' ou letra == 'e' ou letra == 'E' ou letra == 'i' ou letra == 'I' ou letra == 'o' ou letra == 'O' ou letra == 'u' ou letra == 'U' ) {
		    escreva("Sua letra é uma vogal")
		}
		    senao{
		        escreva("Sua letra é uma consoante")
		    }
		
	}
}
